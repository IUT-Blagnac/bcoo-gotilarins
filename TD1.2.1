@startuml
hide circle
hide empty attributes

class Etudiant{
  idNumeroEtudiant
  numero
  nom
  prenom
  dateNaissance
  }
class Examen{
  date
  coefficient
  }
class Enseignant{
  nom
  prenom
  }
class  Cour{
  titre
  code module
  } 
class Noter {
  note
  }
  

Etudiant "*" -- "1..*"Cour
Etudiant "*" -- "*" Examen
Cour "1..*" -- "0..1" Enseignant
Cour "1..1" -- "1..1" Examen
(Etudiant, Examen) . Noter

@enduml
