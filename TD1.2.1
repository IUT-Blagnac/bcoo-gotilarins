@startuml
hide circle
hide empty attributes

class Etudiant{
  idNumeroEtudiant
  numero
  nom
  prenom
  dateNaissance
  }
class Examen{
  date
  coefficient
  }
class Enseignant{
  nom
  prenom
  }
class  Cour{
  titre
  code module
  } 
class Noter {
  note
  }
  

Etudiant "1..*" -- "0..*"Cour
Etudiant "1..*" -- "0..*" Examen
Cour "*" -- "1" Enseignant
Cour  --  Examen
(Etudiant, Examen) . Noter

@enduml
