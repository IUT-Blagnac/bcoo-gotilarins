@startuml
hide circle
hide empty attributes

class Etudiant{
  numero
  nom
  prenom
  dateNaissance
  }
class Examen{
  date
  coefficient
  note
  }
class Enseignant{
  nom
  prenom
  }
class  Cour{
  titre
  code module
  } 
  

Etudiant "*" -- "1..*"Cour
Etudiant "*" -- "*" Examen
Cour "1..*" -- "0..1" Enseignant
Cour "1..1" -- "1..1" Examen

@enduml
